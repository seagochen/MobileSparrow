{
  "GPU_ID": "0",
  "random_seed": 42,
  "cfg_verbose": true,

  "task": "kpts",
  "task_params": {
    "num_joints": 17,
    "export_keypoints": true
  },

  "save_dir": "output/movenet",
  "dataset_root_path": "./data/coco2017_movenet_sp",

  "backbone": "shufflenet_v2",
  "width_mult": 1.5,
  "neck_outc": 96,
  "head_midc": 48,

  "img_size": 192,
  "target_stride": 4,

  "use_color_aug": true,
  "use_flip": true,
  "use_rotate": true,
  "rotate_deg": 12.0,
  "use_scale": true,
  "scale_range": [0.6, 1.25],
  "gaussian_radius": 2,
  "sigma_scale": 1.0,
  "select_person": "largest",

  "pin_memory": true,
  "num_workers": 8,
  "batch_size": 64,
  "epochs": 140,
  "learning_rate": 0.00035,
  "optimizer": "Adam",
  "scheduler": "MultiStepLR-90,130-0.2",
  "weight_decay": 0.0001,
  "clip_gradient": 1.0,
  "log_interval": 10
}
