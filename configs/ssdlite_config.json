{
  "GPU_ID": "0",
  "random_seed": 42,
  "cfg_verbose": true,

  "task": "det",
  "task_params": {
    "class_filter": null
  },

  "save_dir": "output/ssdlite",
  "dataset_root_path": "./data/coco2017_det_5cls",

  "backbone": "mobilenet_v2",
  "width_mult": 1.4,
  "neck_outc": 64,
  "head_midc": 64,

  "img_size": 256,

  "use_color_aug": true,
  "use_flip": true,
  "use_rotate": true,
  "rotate_deg": 30.0,
  "use_scale": true,
  "scale_range": [0.75, 1.25],

  "score_thresh": 0.35,
  "nms_thresh": 0.5,
  "topk": 200,
  "class_agnostic_nms": false,

  "pin_memory": true,
  "num_workers": 8,
  "batch_size": 32,
  "epochs": 100,
  "learning_rate": 0.001,
  "optimizer": "Adam",
  "scheduler": "MultiStepLR-80,95-0.2",
  "weight_decay": 1e-4,
  "clip_gradient": 1.0,
  "reg_alpha": 1.0,
  "log_interval": 10
}
